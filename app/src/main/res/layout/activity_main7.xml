<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Main7Activity">

    <ImageView
        android:id="@+id/vs"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/clean" />

    <Button
        android:id="@+id/vamos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="99dp"
        android:layout_marginBottom="40dp"
        android:fontFamily="@font/aclonica"
        android:text="Go"
        android:textSize="25dp"
        app:layout_constraintBottom_toBottomOf="@+id/vs"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView3" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="161dp"
        android:layout_height="178dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="166dp"
        android:layout_marginBottom="387dp"
        app:layout_constraintBottom_toBottomOf="@+id/vs"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.106"
        app:layout_constraintStart_toStartOf="@+id/vs"
        app:layout_constraintTop_toTopOf="@+id/vs"
        app:srcCompat="@drawable/regice" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="150dp"
        android:layout_height="192dp"
        android:layout_marginStart="245dp"
        android:layout_marginTop="164dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="375dp"
        app:layout_constraintBottom_toBottomOf="@+id/vs"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/vs"
        app:layout_constraintTop_toTopOf="@+id/vs"
        app:srcCompat="@drawable/regice2" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="235dp"
        android:layout_height="117dp"
        android:layout_marginTop="43dp"
        android:layout_marginBottom="571dp"
        android:fontFamily="@font/aclonica"
        android:text="RANDOM SELECTED CHARACTER"
        android:textAlignment="center"
        android:textColor="#FCE833"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/vs"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/vs" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="171dp"
        android:layout_height="31dp"
        android:layout_marginTop="493dp"
        android:layout_marginBottom="208dp"
        android:fontFamily="@font/aclonica"
        android:text="FEATURES"
        android:textColor="#FFEB3B"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="@+id/vs"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/vs" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="193dp"
        android:layout_height="61dp"
        android:layout_marginTop="542dp"
        android:layout_marginBottom="128dp"
        android:text="Regice blah blah (en construcciÃ³n)"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/vs"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/vs" />

    <ImageView
        android:id="@+id/back"
        android:layout_width="80dp"
        android:layout_height="52dp"
        android:layout_marginEnd="331dp"
        android:layout_marginBottom="679dp"
        app:layout_constraintBottom_toBottomOf="@+id/vs"
        app:layout_constraintEnd_toEndOf="@+id/vs"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="?attr/actionModeCloseDrawable" />

</androidx.constraintlayout.widget.ConstraintLayout>